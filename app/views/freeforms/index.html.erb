<p id="notice"><%= notice %></p>

<h1>Freeforms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @freeforms.each do |freeform| %>
      <tr>
        <td><%= link_to 'Show', freeform %></td>
        <td><%= link_to 'Edit', edit_event_freeform_path(freeform) %></td>
        <td><%= link_to 'Destroy', event_freeform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Freeform', new_event_freeform_path %>
